{
  "name": "Aethylred-puppetdashboard",
  "version": "1.0.4",
  "author": "Aaron Hicks <aethylred@gmail.com>",
  "license": "Licensed under GPL3",
  "summary": "A Puppet module to manage the Puppet Dashboard",
  "source": "https://github.com/Aethylred/puppet-puppetdashboard",
  "project_page": "https://github.com/Aethylred/puppet-puppetdashboard",
  "issues_url": "https://github.com/Aethylred/puppet-puppetdashboard/issues",
  "tags": ["puppet", "dashboard"],
  "operatingsystem_support": [
    {
    "operatingsystem": "Ubuntu",
    "operatingsystemrelease": [ "12.04", "14.04" ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">=3.2.0 <5.0.0" },
    { "name": "puppetlabs/apache", "version_requirement": ">=1.0.0 <2.0.0" },
    { "name": "puppetlabs/vcsrepo", "version_requirement": ">=1.0.0 <2.0.0" },
    { "name": "puppetlabs/nodejs", "version_requirement": ">=0.6.0 <1.0.0" },
    { "name": "puppetlabs/ruby", "version_requirement": ">=0.3.0 <1.0.0" },
    { "name": "Aethylred/git", "version_requirement": ">=0.2.0 <1.0.0" }
  ]
}